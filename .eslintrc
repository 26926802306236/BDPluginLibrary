{
	"extends": "eslint:recommended",
	"env": {
        "browser": true,
        "node": true,
        "jquery": false
    },
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "rules": {
        "semi": 2,
		"space-infix-ops": ["error", {"int32Hint": false}]
    },
    "globals": {
        "webpackJsonp": false,
        "Proxy": false,
        "Set": false,
        "BdApi": false,
        "bdPluginStorage": false
    }
}